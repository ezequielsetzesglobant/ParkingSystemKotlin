<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/constraint_layout_item_recycler_view_margin_horizontal"
    android:layout_marginTop="@dimen/constraint_layout_item_recycler_view_margin_top">

    <TextView
        android:id="@+id/text_view_item_recycler_view_start_date"
        style="@style/TextViewItemRecyclerViewStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_item_recycler_view"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_view_item_recycler_view_finish_date"
        style="@style/TextViewItemRecyclerViewStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_item_recycler_view"
        app:layout_constraintTop_toBottomOf="@id/text_view_item_recycler_view_start_date" />

    <TextView
        android:id="@+id/text_view_item_recycler_view_security_code"
        style="@style/TextViewItemRecyclerViewStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_item_recycler_view"
        app:layout_constraintTop_toBottomOf="@id/text_view_item_recycler_view_finish_date" />

    <TextView
        android:id="@+id/text_view_item_recycler_view_place"
        style="@style/TextViewItemRecyclerViewStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_item_recycler_view"
        app:layout_constraintTop_toBottomOf="@id/text_view_item_recycler_view_security_code" />

</androidx.constraintlayout.widget.ConstraintLayout>
